<ActorFrame InitCommand="y,-70;zoom,0.8"><children>
	<Layer
		File="dwi2-selector.png"
		OnCommand="%function(self)
		self:diffusealpha(0)
		end"
		GainFocusCommand="%function(self)
		self:diffuse(1,1,1,0.5)
		end"
		LoseFocusCommand="%function(self)
		self:diffuse(0,0,0,1)
		end"
		/>
		
	<Layer
		Font="Common Normal"
		Text="Return to Options"
		OnCommand="%function(self)
		self:zoom(0.6);
		self:shadowlength(1)
		end"
		/>
	</children></ActorFrame>