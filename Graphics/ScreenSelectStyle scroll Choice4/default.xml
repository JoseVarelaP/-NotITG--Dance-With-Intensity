<ActorFrame
	GainFocusCommand="diffusealpha,1;"
	LoseFocusCommand="diffusealpha,0;"
	SwitchToPage1MessageCommand="visible,true;"
	SwitchToPage2MessageCommand="visible,false;"
	><children>
	<Layer
		File="pad"
		InitCommand="%function(self) self:x( SCREEN_CENTER_X-128 ); self:y( SCREEN_CENTER_Y+92 ); end"
		/>
	<Layer
		File="info"
		InitCommand="%function(self) self:x( SCREEN_CENTER_X+194 ); self:y( SCREEN_CENTER_Y-62 ); end"
		/>
	<Layer
		File="../style4"
		InitCommand="%function(self) self:x( SCREEN_CENTER_X-130 ); self:y( SCREEN_CENTER_Y/2 ); end"
		/>
</children></ActorFrame>