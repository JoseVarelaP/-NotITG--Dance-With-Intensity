<ActorFrame><children>
<Layer Type="AutoActor" File="@THEME:GetPath(EC_BGANIMATIONS,'','CreditsText')" OnCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y" />


<Layer 
	Type="BitmapText"
	Text="Thank you for playing!"
	Font="_sg stroke"
	OnCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y-75;horizalign,left;zoom,0.7;diffusealpha,0;sleep,54.26;queuecommand,ShowLastBit"
	ShowLastBitCommand="%function(self)
	self:linear(2)
	self:diffusealpha(1)
	end" />

<Layer 
	Type="BitmapText"
	Text="DANCE WITH INTENSITY"
	Font="_sui generis"
	OnCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y-40;diffuse,#11f6f3;horizalign,left;zoom,0.55;diffusealpha,0;sleep,54.26;queuecommand,ShowLastBit"
	ShowLastBitCommand="%function(self)
	self:linear(2)
	self:diffusealpha(0.6)
	end" />
<Layer 
	Type="BitmapText"
	Text="Simwolf, 2003."
	Font="_sui generis"
	OnCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y-20;diffuse,#f4f329;horizalign,left;zoom,0.55;diffusealpha,0;sleep,54.26;queuecommand,ShowLastBit"
	ShowLastBitCommand="%function(self)
	self:linear(2)
	self:diffusealpha(0.6)
	end" />
<Layer 
	Type="BitmapText"
	Text="v3.20.00.::  (Recreation of v2.50.00)"
	Font="_sui generis"
	OnCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y+10;horizalign,left;zoom,0.55;diffusealpha,0;sleep,54.26;queuecommand,ShowLastBit"
	ShowLastBitCommand="%function(self)
	self:linear(2)
	self:diffusealpha(0.6)
	end" />
	<Layer 
	Type="BitmapText"
	Text="Visit the original DWI Site!: ::    http://dwi.ddruk.com"
	Font="_sui generis"
	OnCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y+60;horizalign,left;zoom,0.45;diffusealpha,0;sleep,54.26;queuecommand,ShowLastBit"
	ShowLastBitCommand="%function(self)
	self:linear(2)
	self:diffusealpha(0.6)
	end" />


	<Layer 
	Type="Quad"
	OnCommand=";diffusealpha,0;sleep,54.26;queuecommand,ShowLastBit"
	ShowLastBitCommand="%function(self)
	local Pr = PROFILEMAN:GetMachineProfile():GetSaved()
	RandomNumber = math.random(1,2)
        	if Pr.DWIAnnouncer then
		if RandomNumber == 1 then
			SOUND:PlayOnce( THEME:GetPath( EC_SOUNDS, '', 'Internal/credits/pass-00') )
		elseif RandomNumber == 2 then
			SOUND:PlayOnce( THEME:GetPath( EC_SOUNDS, '', 'Internal/credits/pass-04') )
		else
			SOUND:PlayOnce( THEME:GetPath( EC_SOUNDS, '', 'Internal/credits/pass-00') )
		end
	end
	end" />
</children></ActorFrame>