<ActorFrame InitCommand="%function(self)
	
	for i=1,16 do
		_G['Song'..i] = SONGMAN:GetRandomSong();
	end

end"><children>

<Layer
		Type="Quad"
		InitCommand="%function(self)
		Center(self)
		self:diffuse(0,0,0,1)
		self:zoomto(SCREEN_WIDTH+3,SCREEN_HEIGHT)
		end"
		/>

	<Layer
		File="@ThemeFile('dwi2-credits')"
		InitCommand="%function(self)
		self:x(SCREEN_CENTER_X+100)
		self:y(SCREEN_CENTER_Y)
		DWITextureFiltering(self)
		end"
		/>

<Layer File="@THEME:GetPath(EC_BGANIMATIONS,'','CreditsText')" InitCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y" />

<Layer
		Type="Quad"
		InitCommand="%function(self)
		Center(self)
		self:diffuse(0,0,0,1)
		self:zoomto(SCREEN_WIDTH+3,SCREEN_HEIGHT)
		self:fadetop(0.1)
		self:croptop(0.78)
		end"
		/>

	<Layer
		Type="Quad"
		InitCommand="%function(self)
		Center(self)
		self:diffuse(0,0,0,1)
		self:zoomto(SCREEN_WIDTH+3,SCREEN_HEIGHT)
		self:fadebottom(0.1)
		self:cropbottom(0.78)
		end"
		/>

	<Layer
		File="@ThemeFile('dwi2-credits')"
		InitCommand="%function(self)
		self:x(SCREEN_CENTER_X+100)
		self:y(SCREEN_CENTER_Y)
		self:fadetop(0.1)
		self:croptop(0.85)
		DWITextureFiltering(self)
		end"
		/>

	<Layer
		File="@ThemeFile('dwi2-credits')"
		InitCommand="%function(self)
		self:x(SCREEN_CENTER_X+100)
		self:y(SCREEN_CENTER_Y)
		self:fadebottom(0.1)
		self:cropbottom(0.85)
		DWITextureFiltering(self)
		end"
		/>


<Layer 
	Type="BitmapText"
	Text="Thank you for playing!"
	Font="_sg stroke"
	InitCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y-75;horizalign,left;zoom,0.7;diffusealpha,0;sleep,54.26;queuecommand,ShowLastBit"
	ShowLastBitCommand="%function(self)
	self:linear(2)
	self:diffusealpha(1)
	end" />

