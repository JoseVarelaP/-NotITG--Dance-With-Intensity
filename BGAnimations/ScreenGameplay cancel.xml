<ActorFrame><children>
	<!-- Overlay -->
	<Layer
		Type="Quad"
		InitCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;stretchto,0,0,SCREEN_WIDTH,SCREEN_HEIGHT;diffuse,0,0,0,0"
		OnCommand="diffusealpha,0;linear,0.5;diffusealpha,1"
		/>

	<!-- left to right quads -->
	<Layer Type="ActorFrame" OnCommand="addx,-64;linear,1.25;addx,SCREEN_WIDTH*3"><children>
		<Layer Type="Quad"
		InitCommand="zoomto,SCREEN_WIDTH*1.2,32;x,SCREEN_LEFT-64;y,64;diffuse,#000000;horizalign,right"
		/>
		<Layer Type="Quad"
		InitCommand="zoomto,SCREEN_WIDTH*1.2,32;x,SCREEN_LEFT-128;y,128;diffuse,#000000;horizalign,right"
		/>
		<Layer Type="Quad"
		InitCommand="zoomto,SCREEN_WIDTH*1.2,32;x,SCREEN_LEFT-192;y,192;diffuse,#000000;horizalign,right"
		/>
		<Layer Type="Quad"
		InitCommand="zoomto,SCREEN_WIDTH*1.2,32;x,SCREEN_LEFT-192;y,320;diffuse,#000000;horizalign,right"
		/>
		<Layer Type="Quad"
		InitCommand="zoomto,SCREEN_WIDTH*1.2,32;x,SCREEN_LEFT-128;y,384;diffuse,#000000;horizalign,right"
		/>
		<Layer Type="Quad"
		InitCommand="zoomto,SCREEN_WIDTH*1.2,32;x,SCREEN_LEFT-256;y,256;diffuse,#000000;horizalign,right"
		/>
	</children></Layer>

	<Layer Type="ActorFrame" OnCommand="addy,32;addx,0;linear,1.25;addx,SCREEN_WIDTH*-3"><children>
		<Layer Type="Quad"
		InitCommand="zoomto,SCREEN_WIDTH*1.2,32;x,SCREEN_RIGHT+64;y,64;diffuse,#000000;horizalign,left"
		/>
		<Layer Type="Quad"
		InitCommand="zoomto,SCREEN_WIDTH*1.2,32;x,SCREEN_RIGHT+128;y,128;diffuse,#000000;horizalign,left"
		/>
		<Layer Type="Quad"
		InitCommand="zoomto,SCREEN_WIDTH*1.2,32;x,SCREEN_RIGHT+192;y,192;diffuse,#000000;horizalign,left"
		/>
		<Layer Type="Quad"
		InitCommand="zoomto,SCREEN_WIDTH*1.2,32;x,SCREEN_RIGHT+192;y,320;diffuse,#000000;horizalign,left"
		/>
		<Layer Type="Quad"
		InitCommand="zoomto,SCREEN_WIDTH*1.2,32;x,SCREEN_RIGHT+128;y,384;diffuse,#000000;horizalign,left"
		/>
		<Layer Type="Quad"
		InitCommand="zoomto,SCREEN_WIDTH*1.2,32;x,SCREEN_RIGHT+256;y,256;diffuse,#000000;horizalign,left"
		/>
	</children></Layer>

	<!-- Left to right stars -->
	<Layer Type="ActorFrame" OnCommand="addx,-60;addy,0;linear,1.25;addx,SCREEN_WIDTH*3"><children>

		<Layer File="@THEME:GetPath(EC_GRAPHICS,'','r2l')" InitCommand="zoomx,-1;x,SCREEN_LEFT-64;y,64;blend,add" />
		<Layer File="@THEME:GetPath(EC_GRAPHICS,'','r2l')" InitCommand="zoomx,-1;x,SCREEN_LEFT-128;y,128;blend,add" />
		<Layer File="@THEME:GetPath(EC_GRAPHICS,'','r2l')" InitCommand="zoomx,-1;x,SCREEN_LEFT-192;y,192;blend,add" />
		<Layer File="@THEME:GetPath(EC_GRAPHICS,'','r2l')" InitCommand="zoomx,-1;x,SCREEN_LEFT-256;y,256;blend,add" />
		<Layer File="@THEME:GetPath(EC_GRAPHICS,'','r2l')" InitCommand="zoomx,-1;x,SCREEN_LEFT-192;y,320;blend,add" />
		<Layer File="@THEME:GetPath(EC_GRAPHICS,'','r2l')" InitCommand="zoomx,-1;x,SCREEN_LEFT-128;y,384;blend,add" />

	</children></Layer>
	<Layer Type="ActorFrame" OnCommand="addy,31;addx,350;linear,1.25;addx,SCREEN_WIDTH*-3"><children>

		<Layer File="@THEME:GetPath(EC_GRAPHICS,'','r2l')" InitCommand="x,SCREEN_RIGHT-256;y,64;blend,add" />
		<Layer File="@THEME:GetPath(EC_GRAPHICS,'','r2l')" InitCommand="x,SCREEN_RIGHT-192;y,128;blend,add" />
		<Layer File="@THEME:GetPath(EC_GRAPHICS,'','r2l')" InitCommand="x,SCREEN_RIGHT-128;y,192;blend,add" />
		<Layer File="@THEME:GetPath(EC_GRAPHICS,'','r2l')" InitCommand="x,SCREEN_RIGHT-64;y,256;blend,add" />
		<Layer File="@THEME:GetPath(EC_GRAPHICS,'','r2l')" InitCommand="x,SCREEN_RIGHT-128;y,320;blend,add" />
		<Layer File="@THEME:GetPath(EC_GRAPHICS,'','r2l')" InitCommand="x,SCREEN_RIGHT-192;y,384;blend,add" />

	</children></Layer>
	<Layer File="@THEME:GetPath(EC_GRAPHICS,'','GameplayFailure')" InitCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y" />
</children></ActorFrame>