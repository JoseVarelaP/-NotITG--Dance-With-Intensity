            <!-- Game ticks, the difficulty feet that you normally see in SM3.9 and DWI don't exist anymore in OpenITG. Trying to add them via Metrics will do nothing. So, i had to create a system to recreate the way these difficulty ticks worked in DWI. 
                                                                -Jose_Varela    
                    -->
		<Layer
			Font="_sg stroke"
			Text="0"
            Condition="not GAMESTATE:IsCourseMode() and GAMESTATE:IsPlayerEnabled(PLAYER_1)"
			OnCommand="x,SCREEN_CENTER_X-290;y,SCREEN_CENTER_Y+180;zoom,0.5;horizalign,left"
			CurrentSongChangedMessageCommand="sleep,0.01;queuecommand,Update"
			CurrentStepsP1ChangedMessageCommand="sleep,0.01;queuecommand,Update"
			UpdateCommand="%function(self)
            self:hurrytweening(0.01)
       		self:settext('0')
                if GAMESTATE:GetCurrentSong() then
                	self:settext( PMeter(PLAYER_1) )
                	if PMeter(PLAYER_1) > 7 then
                		self:x(SCREEN_CENTER_X-300)
                		self:settext( 'LEVEL     x '..PMeter(PLAYER_1) )
                	else
                		self:x(SCREEN_CENTER_X-300)
                		self:settext( ' ' )
                	end
                else
                    self:settext(' ')
                    self:x(SCREEN_CENTER_X-280)
                    if GAMESTATE:IsPlayerEnabled(PLAYER_1) and GAMESTATE:IsPlayerEnabled(PLAYER_2) then
                    	self:settext(' ')
                    end
                end
        end"
			/>

        <Layer
            File="@ThemeFile('Ticks')"
            Condition="not GAMESTATE:IsCourseMode() and GAMESTATE:IsPlayerEnabled(PLAYER_1)"
            OnCommand="x,SCREEN_CENTER_X-270;y,SCREEN_CENTER_Y+183;horizalign,left"
            CurrentSongChangedMessageCommand="sleep,0.01;queuecommand,Update"
            CurrentStepsP1ChangedMessageCommand="sleep,0.01;queuecommand,Update"
            UpdateCommand="%function(self)
            self:hurrytweening(0.01)
            self:diffusealpha(0)
                if GAMESTATE:GetCurrentSong() then
                    self:diffuse(0,0,0,1)
                        if PMeter(PLAYER_1) > 7 then
                            self:diffusealpha(0)
                        else
                            self:cropright(0)
                            self:diffusealpha(1)
                            self:x(SCREEN_CENTER_X-300)
                        end
                else
                    self:diffuse(1,1,1,0)
                    self:cropright(1)
                    self:x(SCREEN_CENTER_X-300)
                end
        end"
            />
		<Layer
			File="@ThemeFile('Ticks')"
			Condition="not GAMESTATE:IsCourseMode() and GAMESTATE:IsPlayerEnabled(PLAYER_1)"
			OnCommand="x,SCREEN_CENTER_X-270;y,SCREEN_CENTER_Y+183;horizalign,left"
			CurrentSongChangedMessageCommand="sleep,0.01;queuecommand,Update"
			CurrentStepsP1ChangedMessageCommand="sleep,0.01;queuecommand,Update"
			UpdateCommand="%function(self)
            self:hurrytweening(0.01)
       		self:diffusealpha(0)
                if GAMESTATE:GetCurrentSong() then
                	self:diffusealpha(1)
                	if PMeter(PLAYER_1) == 1 then
                		self:cropright(0.89)
                		self:x(SCREEN_CENTER_X-300)
                	elseif PMeter(PLAYER_1) == 2 then
                		self:cropright(0.7)
                		self:x(SCREEN_CENTER_X-300)
                	elseif PMeter(PLAYER_1) == 3 then
                		self:cropright(0.6)
                		self:x(SCREEN_CENTER_X-300)
                	elseif PMeter(PLAYER_1) == 4 then
                		self:cropright(0.42)
                		self:x(SCREEN_CENTER_X-300)
                	elseif PMeter(PLAYER_1) == 5 then
                		self:cropright(0.3)
                		self:x(SCREEN_CENTER_X-300)
                	elseif PMeter(PLAYER_1) == 6 then
                		self:cropright(0.15)
                		self:x(SCREEN_CENTER_X-300)
                	elseif PMeter(PLAYER_1) == 7 then
                		self:cropright(0)
                		self:x(SCREEN_CENTER_X-300)
                	elseif PMeter(PLAYER_1) > 7 then
                		self:cropright(0.9)
                		self:x(SCREEN_CENTER_X-245)
                	end
                		if PDiff(PLAYER_1) == 4 then
                			self:diffuse(1,0.5,1,1)
                		elseif PDiff(PLAYER_1) == 3 then
                			self:diffuse(0,1,0,1)
                		elseif PDiff(PLAYER_1) == 2 then
                			self:diffuse(1,0,0.5,1)
                		elseif PDiff(PLAYER_1) == 1 then
                			self:diffuse(1,1,0,1)
                		elseif PDiff(PLAYER_1) == 0 then
                			self:diffuse(0,1,1,1)
                		else
                			self:diffuse(1,1,1,1)
                		end
                else
                    self:diffuse(1,1,1,1)
                    self:cropright(1)
                    self:x(SCREEN_CENTER_X-300)
                end
        end"
			/>















