            CurrentSongChangedMessageCommand="sleep,0.01;queuecommand,Update"
            CurrentStepsP2ChangedMessageCommand="sleep,0.01;queuecommand,Update"
            UpdateCommand="%function(self)
            local Pr = PROFILEMAN:GetMachineProfile():GetSaved()
            self:hurrytweening(0.01)
            self:diffusealpha(0)
                    if OPENITG or FUCK_EXE then
                        if GAMESTATE:GetCurrentSong() then
                            self:diffusealpha(1)
                            self:settext( GAMESTATE:GetCurrentSong():GetDisplayArtist() ..' //' )
                            self:maxwidth(400*((SCREEN_WIDTH/SCREEN_HEIGHT)/(4/3)))
                        else
                            self:settext(' ')
                            self:diffusealpha(0)
                        end
                    else
                        self:hidden(1)
                    end
            end"
