<ActorFrame><children>
	<!-- Overlay -->
		<Layer Type="ActorFrame" OnCommand="x,0;y,SCREEN_CENTER_Y-190"><children>
			<Layer
			Type="Quad"
			InitCommand="horizalign,center;stretchto,0,0,SCREEN_WIDTH,450;diffuse,0,0,0,0;diffusealpha,1;sleep,0.1;diffusealpha,0"
			/>
		</children></Layer>

	<!-- left to right quads -->
	<Layer Type="ActorFrame" OnCommand="addx,64;addy,30;linear,1.25;addx,SCREEN_WIDTH*-3"><children>
		<Layer Type="Quad"
		InitCommand="zoomto,SCREEN_WIDTH*1.2,32;x,SCREEN_RIGHT+64;y,64;diffuse,#000000;horizalign,right"
		/>
		<Layer Type="Quad"
		InitCommand="zoomto,SCREEN_WIDTH*1.2,32;x,SCREEN_RIGHT+128;y,128;diffuse,#000000;horizalign,right"
		/>
		<Layer Type="Quad"
		InitCommand="zoomto,SCREEN_WIDTH*1.2,32;x,SCREEN_RIGHT+192;y,192;diffuse,#000000;horizalign,right"
		/>
		<Layer Type="Quad"
		InitCommand="zoomto,SCREEN_WIDTH*1.2,32;x,SCREEN_RIGHT+192;y,320;diffuse,#000000;horizalign,right"
		/>
		<Layer Type="Quad"
		InitCommand="zoomto,SCREEN_WIDTH*1.2,32;x,SCREEN_RIGHT+128;y,384;diffuse,#000000;horizalign,right"
		/>
		<Layer Type="Quad"
		InitCommand="zoomto,SCREEN_WIDTH*1.2,32;x,SCREEN_RIGHT+256;y,256;diffuse,#000000;horizalign,right"
		/>
	</children></Layer>

	<!-- right to left quads -->
	<Layer Type="ActorFrame" OnCommand="addy,-2;addx,-60;linear,1.25;addx,SCREEN_WIDTH*3"><children>
		<Layer Type="Quad"
		InitCommand="zoomto,SCREEN_WIDTH*1.2,32;x,SCREEN_LEFT-64;y,64;diffuse,#000000;horizalign,left"
		/>
		<Layer Type="Quad"
		InitCommand="zoomto,SCREEN_WIDTH*1.2,32;x,SCREEN_LEFT-128;y,128;diffuse,#000000;horizalign,left"
		/>
		<Layer Type="Quad"
		InitCommand="zoomto,SCREEN_WIDTH*1.2,32;x,SCREEN_LEFT-192;y,192;diffuse,#000000;horizalign,left"
		/>
		<Layer Type="Quad"
		InitCommand="zoomto,SCREEN_WIDTH*1.2,32;x,SCREEN_LEFT-192;y,320;diffuse,#000000;horizalign,left"
		/>
		<Layer Type="Quad"
		InitCommand="zoomto,SCREEN_WIDTH*1.2,32;x,SCREEN_LEFT-128;y,384;diffuse,#000000;horizalign,left"
		/>
		<Layer Type="Quad"
		InitCommand="zoomto,SCREEN_WIDTH*1.2,32;x,SCREEN_LEFT-256;y,256;diffuse,#000000;horizalign,left"
		/>
	</children></Layer>

	<!-- Left to right stars -->
	<Layer Type="ActorFrame" OnCommand="addx,-80;addy,-2;linear,1.25;addx,SCREEN_WIDTH*3"><children>

		<Layer File="@THEME:GetPath(EC_GRAPHICS,'','r2l')" InitCommand="zoom,1.1;zoomx,-1.1;x,SCREEN_LEFT-64;y,64" />
		<Layer File="@THEME:GetPath(EC_GRAPHICS,'','r2l')" InitCommand="zoom,1.1;zoomx,-1.1;x,SCREEN_LEFT-128;y,128" />
		<Layer File="@THEME:GetPath(EC_GRAPHICS,'','r2l')" InitCommand="zoom,1.1;zoomx,-1.1;x,SCREEN_LEFT-192;y,192" />
		<Layer File="@THEME:GetPath(EC_GRAPHICS,'','r2l')" InitCommand="zoom,1.1;zoomx,-1.1;x,SCREEN_LEFT-256;y,256" />
		<Layer File="@THEME:GetPath(EC_GRAPHICS,'','r2l')" InitCommand="zoom,1.1;zoomx,-1.1;x,SCREEN_LEFT-192;y,320" />
		<Layer File="@THEME:GetPath(EC_GRAPHICS,'','r2l')" InitCommand="zoom,1.1;zoomx,-1.1;x,SCREEN_LEFT-128;y,384" />

	</children></Layer>
	<Layer Type="ActorFrame" OnCommand="addy,30;addx,400;linear,1.25;addx,SCREEN_WIDTH*-3"><children>

		<Layer File="@THEME:GetPath(EC_GRAPHICS,'','r2l')" InitCommand="zoom,1.1;x,SCREEN_RIGHT-256;y,64" />
		<Layer File="@THEME:GetPath(EC_GRAPHICS,'','r2l')" InitCommand="zoom,1.1;x,SCREEN_RIGHT-192;y,128" />
		<Layer File="@THEME:GetPath(EC_GRAPHICS,'','r2l')" InitCommand="zoom,1.1;x,SCREEN_RIGHT-128;y,192" />
		<Layer File="@THEME:GetPath(EC_GRAPHICS,'','r2l')" InitCommand="zoom,1.1;x,SCREEN_RIGHT-64;y,256" />
		<Layer File="@THEME:GetPath(EC_GRAPHICS,'','r2l')" InitCommand="zoom,1.1;x,SCREEN_RIGHT-128;y,320" />
		<Layer File="@THEME:GetPath(EC_GRAPHICS,'','r2l')" InitCommand="zoom,1.1;x,SCREEN_RIGHT-192;y,384" />

	</children></Layer>
	<Layer
		File="@THEME:GetPath(EC_GRAPHICS,'','GameplayReady')"
		OnCommand="x,SCREEN_CENTER_X;y,SCREEN_CENTER_Y;sleep,2;diffusealpha,0"
		/>
	<Layer
        Type="Quad"
        OnCommand="queuecommand,PlaySound"
        PlaySoundCommand="%function(self)
        StartNumber = math.random(1,3)
       	local Pr = PROFILEMAN:GetMachineProfile():GetSaved()
        if Pr.DWIAnnouncer then
        	if StartNumber == 1 then
        		SOUND:PlayOnce( THEME:GetPath( EC_SOUNDS, '', 'Internal/ready/song') )
        	elseif StartNumber == 2 then
        		SOUND:PlayOnce( THEME:GetPath( EC_SOUNDS, '', 'Internal/ready/song-05') )
        	elseif StartNumber == 3 then
        		SOUND:PlayOnce( THEME:GetPath( EC_SOUNDS, '', 'Internal/ready/song-17') )
        	else
        		SOUND:PlayOnce( THEME:GetPath( EC_SOUNDS, '', 'Internal/ready/song') )
        	end
    	end
        end"
        />
</children></ActorFrame>